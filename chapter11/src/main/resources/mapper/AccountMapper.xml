<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fatal.mapper.AccountMapper">

    <update id="update">
        UPDATE account
        SET money = #{money}
        WHERE id = #{id}
    </update>

</mapper>